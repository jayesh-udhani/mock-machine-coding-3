package models;
import java.util.UUID;

public class User {
	private UUID userId;
	private String name;
	private String email;
	
	public User(String name,String email) {
		this.userId = UUID.randomUUID();
		this.name = name;
		this.email = email;
	}
	
	public UUID getId() {
		return this.userId;
	}
	
	public String getName() {
		return this.name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public String getEmail() {
		return this.email;
	}
	
}
